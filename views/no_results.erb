          <h2>Create a new Waddle:</h2>
          <hr>
        </div>
            <form method="POST" action="/create_groups">
              <h4>Start:</h4>
              <input type="text" name="location">
              <h4>Destination:</h4>
              <input type="text" name="destination">
              <h4>Name:</h4>
              <input type="text" name="name">
              <h4>Time:</h4>
              <input type="text" name="time">
              <h4>Method of Transport:</h4>
              <input type="text" name="method">
              <h4></h4>
              <input class = "btn btn-default" type="submit">
              <hr>
            </form>
          </div>
        </div>
        <div clas = "row">
          <a href = "/show_all" class = "btn btn-xl btn-outline">See All Waddles</a>
